{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MyBGList": "Information"
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Warning",
        "Override": {
          "MyBGList": "Information"
        }
      }
    },
    "JWT": {
      "Issuer": "MyBGList",
      "Audience": "MyBGList",
      "SigningKey": "MyVeryOwnTestSigningKey123$"
    },
    "Console": {
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*",
  "UseDeveloperExceptionPage": false
}
